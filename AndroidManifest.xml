<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anhanguera.cadusuario"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tuxdca"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.anhanguera.cadusuario.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.anhanguera.cadusuario.InsereUsuarioActivity"
            android:label="@string/title_activity_insere_usuario" >
        </activity>
        <activity
            android:name="com.anhanguera.cadusuario.AlteraUsuarioActivity"
            android:label="@string/title_activity_altera_usuario" >
        </activity>
        <activity
            android:name="com.anhanguera.cadusuario.SelecionaUsuarioActivity"
            android:label="@string/title_activity_seleciona_usuario" >
        </activity>
    </application>

</manifest>
